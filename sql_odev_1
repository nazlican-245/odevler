SELECT title,description from film;

SELECT length FROM film WHERE film BETWEEN 65 AND 70;

SELECT * FROM film WHERE rental_rate = 0.99 AND replacement_cost = 12.99 OR 28.99;

SELECT first_name, last_name FROM customer WHERE first_name = 'Mary';

SELECT length,rental_rate FROM film WHERE NOT length >50 AND rental_rate BETWEEN 2.99 AND 4.99;
